{
  "name": "andersonef/ghcomponent-animal",
  "description": "Componentes Girolando - Animal",
  "keywords": [
    "laravel",
    "girolando",
    "componente",
    "animal"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Anderson Nunes",
      "email": "anderson.nuneseth@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support" : "5.1.x",
    "andersonef/gh-base-component" : "1.0.x",
    "andersonef/api-clientlayer": "^1.2",
    "illuminate/routing": "5.1.x",
    "yajra/laravel-datatables-oracle": "~5.0",
    "andersonef/repositories-pattern": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Andersonef\\Componentes\\Animal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andersonef\\Tests\\Componentes\\Animal\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

